#!/usr/bin/env bash
case $(uname) in
  Linux)
    emacsclient=/usr/bin/emacsclient
    ;;
  Darwin)
    emacsclient=/etc/profiles/per-user/torgeir/bin/emacsclient
    ;;
esac

if [ -n $INSIDE_EMACS ]; then
  $emacsclient -s torgemacs -a -c -q "$@"
else
  $emacsclient -s torgemacs -a "" -t "$@" # 2> /dev/null
fi
